{
    "name": "beloved/fcm",
    "description": "FCM (Firebase Cloud Messaging) Notifications Driver for Laravel",
    "homepage": "https://github.com/Beloved46/fcm",
    "license": "MIT",
    "authors": [
      {
        "name": "Chris Bautista",
        "email": "chris.bautista@coreproc.ph",
        "homepage": "https://coreproc.com",
        "role": "Developer"
      }
    ],
    "require": {
      "php": "^8.2|^8.3|^8.4|^8.5",
      "guzzlehttp/guzzle": "^7.0",
      "illuminate/notifications": "^11.0|^12.0",
      "illuminate/support": "^11.0|^12.0",
      "kreait/laravel-firebase": "^6.0"
    },
    "require-dev": {
      "mockery/mockery": "^1.6.0",
      "phpunit/phpunit": "^11.0"
    },
    "autoload": {
      "psr-4": {
        "NotificationChannels\\Fcm\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "NotificationChannels\\Fcm\\Test\\": "tests"
      }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "config": {
      "sort-packages": true
    }
  }
  